{
    "name": "holt59/cakephp3-bootstrap-helpers",
    "description": "Bootstrap Helpers for CakePHP 3.0",
    "keywords": ["CakePHP", "Bootstrap"],
    "license": "MIT",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "<6.0",
        "cakephp/cakephp-codesniffer": "~2.1"
    },
    "autoload": {
        "psr-4": {
            "Bootstrap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bootstrap\\Test\\": "tests"
        }
    },
    "extra": {
        "installer-name": "Bootstrap"
    }
}
